<h1>Listing purchase_histories</h1>

<table>
  <tr>
    <th>Symbol</th>
    <th>Quantity</th>
    <th>Purchase price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @purchase_histories.each do |purchase_history| %>
  <tr>
    <td><%= purchase_history.symbol %></td>
    <td><%= purchase_history.quantity %></td>
    <td><%= purchase_history.purchase_price %></td>
    <td><%= link_to 'Show', purchase_history %></td>
    <td><%= link_to 'Edit', edit_purchase_history_path(purchase_history) %></td>
    <td><%= link_to 'Destroy', purchase_history, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Purchase history', new_purchase_history_path %>
